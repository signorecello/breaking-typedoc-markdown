{
  "compilerOptions": {
    "target": "esnext",
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./lib",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": false,
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"],
}
